<div class="row">
  <div class="col-sm-3">
    <%= render 'layouts/main_tab' %>
    <table class="table table-bordered">
      <% @folders.each do |folder| %>
        <tr>
          <td><%= folder.name %></td>
          <td><%= link_to '詳細', folder %></td>
